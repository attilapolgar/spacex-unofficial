{
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "plugins": ["react", "react-native", "eslint-plugin-import"],
  "rules": {
    "no-console": "warn"
  },
  "env": {
    "react-native/react-native": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  }
}
